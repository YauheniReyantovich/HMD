<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.reyantovich.yauheni.hmdbase.HmdRefs" table="HMD_REFS">
        <meta attribute="class-description">
            This class contains references between objects.
        </meta>
        <composite-id>
            <key-many-to-one name="attribute" column="attr_id"/>
            <key-many-to-one name="object" column="object_id"/>
            <key-many-to-one name="reference" column="ref"/>
        </composite-id>
    </class>
</hibernate-mapping>